        {
          "name": "user",
          "type": "user",
          "required": false
        }
      ],
      "cooldown_seconds": 3,
      "actions": [
        {
          "action": "economy.get_balance",
          "from": "args.user|author",
          "assign": "balance"
        },
        {
          "action": "discord.reply_embed",
          "title": "Your Balance",
          "description": "Balance: {{balance}} {{economy.currency_name}}"
        }
      ]
    },
    {
      "name": "shop",
