        {
          "name": "item",
          "type": "string",
          "required": true
        },
        {
          "name": "qty",
          "type": "integer",
          "required": false,
          "default": 1
        },
        {
          "name": "map",
          "type": "string",
          "required": false
        }
      ],
      "cooldown_seconds": 3,
      "actions": [
        {
          "action": "shop.verify_item_exists",
          "item_from": "args.item"
        },
        {
          "action": "economy.charge",
          "amount_from": "shop.item_price * args.qty",
          "reason": "Shop purchase: {{args.item}}"
        },
        {
          "action": "rcon.give_item",
          "item_from": "args.item",
          "quantity_from": "args.qty",
          "map_from": "args.map",
          "to_player": "author"
        },
        {
          "action": "discord.reply_embed",
          "title": "\u2705 Purchase Complete",
          "description": "Bought {{args.qty}}x {{args.item}} for {{computed.total_price}} {{economy.currency_name}}."
        }
      ]
    },
    {
      "name": "transfer",
      "description": "Transfer points to another player.",
      "options_schema": [
        {
          "name": "to",
          "type": "user",
          "required": true
        },
        {
          "name": "amount",
          "type": "integer",
          "required": true
        }
      ],
      "cooldown_seconds": 5,
      "actions": [
        {
          "action": "economy.transfer",
          "from_player": "author",
          "to_player_from": "args.to",
          "amount_from": "args.amount"
        },
        {
          "action": "discord.reply_embed",
          "title": "\ud83d\udcb8 Transfer Complete",
          "description": "Sent {{args.amount}} {{economy.currency_name}} to {{args.to}}."
        }
      ]
    },
